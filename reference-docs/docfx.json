{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml",
          "**/includes/**"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg",
          "**/includes/media/**"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/bi-reference/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None",
      "titleSuffix": "BI"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "AsRefDocs"
  }
}
